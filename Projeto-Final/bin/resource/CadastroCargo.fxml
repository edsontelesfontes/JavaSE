<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.cursojava.projetofinal.ui.CadastroCargoController">
   <center>
      <AnchorPane BorderPane.alignment="CENTER">
         <children>
            <Label layoutX="14.0" layoutY="14.0" text="Cargo:" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0" />
            <TextField fx:id="txtNome" layoutX="14.0" layoutY="35.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0" />
            <Button layoutX="14.0" layoutY="87.0" mnemonicParsing="false" onAction="#salvarOnAction" prefHeight="32.0" prefWidth="99.0" text="Salvar" />
            <Button layoutX="130.0" layoutY="87.0" mnemonicParsing="false" onAction="#limparOnAction" prefHeight="32.0" prefWidth="99.0" text="Limpar" />
         </children>
      </AnchorPane>
   </center>
   <bottom>
      <AnchorPane>
         <children>
            <Label fx:id="lblMensagem" BorderPane.alignment="CENTER" />
         </children>
      </AnchorPane>
   </bottom>
</BorderPane>
